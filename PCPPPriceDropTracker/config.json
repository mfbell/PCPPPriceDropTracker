{
  "project": {
    "mode": "GUI-only",
    "update": "true"
  },
  "databases": {
    "update": [
      "./PCPPPriceDropTracker/pcpp_offers.sqlite3"
    ],
    "default": "./PCPPPriceDropTracker/pcpp_offers.sqlite3",
    "resent": [
      "./PCPPPriceDropTracker/pcpp_offers.sqlite3",
      "./PCPPPriceDropTracker/main.db"
    ]
  },
  "global_db_settings": {
    "updating": {
      "wait": 21600
    },
    "notifications": {
      "method": "null",
      "notify": "immediately",
      "renotify_after_updating": "false",
      "renotify_after_time": 0
    }
  },
  "logging": {
    "path": "./logs/",
    "filename": "%Y-%m-%d %H-%M-%S.log",
    "level": "DEBUG"
  }
}
